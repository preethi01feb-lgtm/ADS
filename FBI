#include <stdio.h>

void fibonacci_iterative(int n) {
    int a = 0, b = 1, c, i;

    printf("Fibonacci Series: ");

    if (n >= 1)
        printf("%d ", a);
    if (n >= 2)
        printf("%d ", b);

    for (i = 3; i <= n; i++) {
        c = a + b;
        printf("%d ", c);
        a = b;
        b = c;
    }
}

int main() {
    int n;

    printf("Enter how many terms: ");
    scanf("%d", &n);

    fibonacci_iterative(n);

    return 0;
}
