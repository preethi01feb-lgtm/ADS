#include <stdio.h>

#define MAX 20

int visited[MAX];

// DFS function
void DFS(int adj[MAX][MAX], int n, int v) {
    int i;

    visited[v] = 1;
    printf("%d ", v);

    // Visit all adjacent vertices
    for (i = 0; i < n; i++) {
        if (adj[v][i] == 1 && visited[i] == 0) {
            DFS(adj, n, i);
        }
    }
}

int main() {
    int adj[MAX][MAX];
    int n, i, j, start;

    printf("Enter number of vertices: ");
    scanf("%d", &n);

    printf("Enter adjacency matrix:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            scanf("%d", &adj[i][j]);
        }
    }

    printf("Enter starting vertex: ");
    scanf("%d", &start);

    // Initially mark all nodes unvisited
    for (i = 0; i < n; i++)
        visited[i] = 0;

    printf("DFS Traversal: ");
    DFS(adj, n, start);

    return 0;
}
